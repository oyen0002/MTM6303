{%- capture product -%}{{-all_products['beige-eiffel-chair'] | json-}}{%- endcapture -%}{%- if product contains 'Liquid error' -%}window.GloboMenuConfig.GloboMenuMissingProducts.push('beige-eiffel-chair');{%- else -%}window.GloboMenuConfig.products['beige-eiffel-chair'] = {{-product-}};{%- endif -%}
{%- capture product -%}{{-all_products['indoor-chairs'] | json-}}{%- endcapture -%}{%- if product contains 'Liquid error' -%}window.GloboMenuConfig.GloboMenuMissingProducts.push('indoor-chairs');{%- else -%}window.GloboMenuConfig.products['indoor-chairs'] = {{-product-}};{%- endif -%}
window.GloboMenuConfig.pages['contact'] = {handle:{{-pages['contact'].handle | json-}},title:{{-pages['contact'].title | json-}}};window.GloboMenuConfig.pages['about-us'] = {handle:{{-pages['about-us'].handle | json-}},title:{{-pages['about-us'].title | json-}}};</script>
